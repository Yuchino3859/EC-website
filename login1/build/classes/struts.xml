<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.3//EN" "http://struts.apache.org/dtds/struts-2.3.dtd"> <!-- Strutsの詳細設定 -->
<struts>
	<constant name="struts.devMode" value="true"/>
	<package name="com.internousdev.login1.action" extends="struts-default">


<!-- GoHomeAction -->
<action name="GoHomeAction" class="com.internousdev.login1.action.GoHomeAction" method="execute">
<result name="success">home.jsp</result>
</action>

<!-- HomeAction -->
<action name="HomeAction" class="com.internousdev.login1.action.HomeAction" method="execute">
<result name="success">login.jsp</result>
</action>

<!-- LoginAction -->
<action name="LoginAction" class="com.internousdev.login1.action.LoginAction" method="execute">
<result name="success">success.jsp</result>
<result name="error">login.jsp</result>
</action>

</package>
</struts>